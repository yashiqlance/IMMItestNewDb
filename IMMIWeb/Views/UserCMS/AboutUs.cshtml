@model IMMIWeb.Cm


<section class="main-tab-section mt-90 text-center">
    <div class="container">
        <div class="row" style="align-items:center; justify-content:center;">

            <div class="col-lg-8">
                <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane app-lang-wrap fade" id="nav-app-language" role="tabpanel" aria-labelledby="nav-app-language-tab">
                    </div>
                    <div class="tab-pane fade about-wrap active show" id="nav-about" role="tabpanel" aria-labelledby="nav-about-tab">
                        <div class="about-inner text-center">
                            <div class="title text-center">
                                <h2>About Us</h2>
                            </div><h5>@Html.Raw(Html.Raw(Model.Description))</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>