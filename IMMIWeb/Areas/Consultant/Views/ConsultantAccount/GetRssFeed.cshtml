@using Nancy.Helpers;
@model IEnumerable<System.ServiceModel.Syndication.SyndicationItem>

<div class="container" style="margin-top: 120px;">
    <div class="row">

        @foreach (var item in Model)
        {
            <h2><a href="@item.Links[0].Uri.ToString()" target="_blank">@item.Title.Text</a></h2>
            <p>@item.PublishDate.ToLocalTime()</p>
            <p>@Html.Raw(item.Summary.Text)</p>
            <hr />
        }

    </div>
</div>

